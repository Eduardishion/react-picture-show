{
  "name": "react-picture-show",
  "version": "1.4.0",
  "description": "Slideshow Component for React.js",
  "main": "lib/PictureShow.js",
  "scripts": {
    "test": "mocha --compilers js:babel/register test/PictureShow.test.jsx",
    "test-watch": "mocha -G -w -R min -r should --compilers js:babel/register test/PictureShow.test.jsx",
    "build:css": "node-sass src/PictureShow.scss lib/PictureShow.css",
    "build:js": "babel ./src --out-dir ./lib",
    "build": "npm run build:js && npm run build:css",
    "prepublish": "npm run build",
    "babel-node": "babel-node --stage 0 --ignore='foo|bar|baz'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/areusjs/react-picture-show.git"
  },
  "keywords": [
    "react-component",
    "slideshow",
    "carousel"
  ],
  "author": "gregoryskiano@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/areusjs/react-picture-show/issues"
  },
  "homepage": "https://github.com/areusjs/react-picture-show",
  "devDependencies": {
    "b": "^2.0.1",
    "babel": "^5.8.21",
    "babel-eslint": "^4.0.5",
    "babel-jest": "^5.3.0",
    "babelify": "^6.1.3",
    "balanced-match": "^0.2.0",
    "browserify": "^11.0.1",
    "common-errors": "^0.4.18",
    "compression": "^1.4.1",
    "connect-cache-control": "^1.0.0",
    "express": "^4.11.2",
    "gulp": "*",
    "gulp-buffer": "0.0.2",
    "gulp-livereload": "^3.7.0",
    "gulp-nodemon": "^1.0.5",
    "gulp-rev": "^3.0.1",
    "gulp-sass": "*",
    "gulp-uglify": "^1.1.0",
    "gulp-util": "^3.0.3",
    "jsdom": "^3.1.2",
    "mocha": "^2.2.5",
    "morgan": "^1.5.1",
    "node-jsx": "^0.13.3",
    "proxyquire": "^1.3.2",
    "reactify": "^1.1.1",
    "rimraf": "^2.2.8",
    "serve-static": "^1.8.1",
    "should": "^5.0.1",
    "sinon": "^1.15.4",
    "vinyl-source-stream": "^1.0.0",
    "watchify": "^2.3.0",
    "node-sass": "*"
  },
  "dependencies": {
    "lodash": "^3.3.1",
    "node-sass": "*",
    "react": ">=0.11.2 <1.0.0",
    "react-dom": "^0.14.7",
    "react-swipeable": "^2.0.0"
  }
}
